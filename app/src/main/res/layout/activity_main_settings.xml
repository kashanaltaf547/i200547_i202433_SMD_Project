<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".MainActivity5_Settings">


    <ImageView
        android:id="@+id/e1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="-15dp"
        android:src="@drawable/vector1" />

    <ImageView
        android:id="@+id/e2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:src="@drawable/vector2" />


    <ImageView
        android:id="@+id/e3"
        android:layout_width="314dp"
        android:layout_height="316dp"
        android:layout_below="@id/e1"
        android:layout_marginLeft="-45dp"
        android:layout_marginTop="-10dp"
        android:src="@drawable/vector3" />

    <TextView
        android:id="@+id/e4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="90dp"
        android:text="Settings"
        android:textColor="@color/black"
        android:textSize="30sp"
        android:textStyle="bold" />

    <ImageButton
        android:id="@+id/e5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="5dp"
        android:background="@color/transparent"
        android:contentDescription="Button to go back"
        android:src="@drawable/img" />

    <TextView
        android:id="@+id/e6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/e4"
        android:text="Account"
        android:textStyle="bold"
        android:textSize="18sp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="15dp" />


    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/e7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:cardElevation="10dp"
        android:layout_below="@id/e6"
        android:layout_marginLeft="45dp"
        android:layout_marginRight="45dp">

        <LinearLayout
            android:id="@+id/e8"
            android:layout_width="match_parent"
            android:weightSum="1"
            android:orientation="horizontal"
            android:background="@color/semi_gray_color"
            android:layout_height="45dp">

            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:id="@+id/e9"
                android:layout_marginLeft="6dp"
                android:src="@drawable/image4"
                android:layout_weight="0.1"/>

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="0.2"
                android:id="@+id/e10"
                android:backgroundTint="@color/transparent"
                android:text="Profile"
                android:textColor="@color/black"
                android:textSize="15sp"
                tools:ignore="TouchTargetSizeCheck"/>

        </LinearLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/e11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:cardElevation="10dp"
        android:layout_below="@id/e7"
        android:layout_marginLeft="45dp"
        android:layout_marginRight="45dp">


        <LinearLayout
            android:id="@+id/e12"
            android:layout_width="match_parent"
            android:weightSum="1"
            android:orientation="horizontal"
            android:background="@color/semi_gray_color"
            android:layout_height="45dp">

            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginLeft="3dp"
                android:id="@+id/e13"
                android:src="@drawable/image3"
                android:layout_weight="0.1"/>

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="0.2"
                android:id="@+id/e14"
                android:backgroundTint="@color/transparent"
                android:text="Security"
                android:textColor="@color/black"
                android:textSize="15sp"
                tools:ignore="TouchTargetSizeCheck"/>

        </LinearLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/e15"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:cardElevation="10dp"
        android:layout_below="@id/e11"
        android:layout_marginLeft="45dp"
        android:layout_marginRight="45dp">


        <LinearLayout
            android:id="@+id/e16"
            android:layout_width="match_parent"
            android:weightSum="1"
            android:orientation="horizontal"
            android:background="@color/semi_gray_color"
            android:layout_height="45dp">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/e17"
                android:src="@drawable/baseline_notifications_none_24"
                android:layout_weight="0.1"/>

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="0.2"
                android:id="@+id/e18"
                android:backgroundTint="@color/transparent"
                android:text="Notifications"
                android:textColor="@color/black"
                android:textSize="15sp"
                tools:ignore="TouchTargetSizeCheck"/>

        </LinearLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/e19"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:cardElevation="10dp"
        android:layout_below="@id/e15"
        android:layout_marginLeft="45dp"
        android:layout_marginRight="45dp">


        <LinearLayout
            android:id="@+id/e20"
            android:layout_width="match_parent"
            android:weightSum="1"
            android:orientation="horizontal"
            android:background="@color/semi_gray_color"
            android:layout_height="45dp">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/e21"
                android:src="@drawable/baseline_notifications_none_24"
                android:layout_weight="0.1"/>

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="0.2"
                android:id="@+id/e22"
                android:backgroundTint="@color/transparent"
                android:text="Privacy"
                android:textColor="@color/black"
                android:textSize="15sp"
                tools:ignore="TouchTargetSizeCheck"/>

        </LinearLayout>

    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/e23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/e19"
        android:text="Support &amp; About"
        android:textStyle="bold"
        android:textSize="18sp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="15dp" />

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/e24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:cardElevation="10dp"
        android:layout_below="@id/e23"
        android:layout_marginLeft="45dp"
        android:layout_marginRight="45dp">


        <LinearLayout
            android:id="@+id/e25"
            android:layout_width="match_parent"
            android:weightSum="1"
            android:orientation="horizontal"
            android:background="@color/semi_gray_color"
            android:layout_height="45dp">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/e26"
                android:src="@drawable/baseline_help_outline_24"
                android:layout_weight="0.1"/>

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="0.2"
                android:id="@+id/e27"
                android:backgroundTint="@color/transparent"
                android:text="Help &amp; Support"
                android:textColor="@color/black"
                android:textSize="15sp"
                tools:ignore="TouchTargetSizeCheck"/>

        </LinearLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/e28"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:cardElevation="10dp"
        android:layout_below="@id/e24"
        android:layout_marginLeft="45dp"
        android:layout_marginRight="45dp">


        <LinearLayout
            android:id="@+id/e29"
            android:layout_width="match_parent"
            android:weightSum="1"
            android:orientation="horizontal"
            android:background="@color/semi_gray_color"
            android:layout_height="45dp">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/e30"
                android:src="@drawable/baseline_person_24"
                android:layout_weight="0.1"/>

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="0.2"
                android:id="@+id/e31"
                android:backgroundTint="@color/transparent"
                android:text="Terms and Policies"
                android:textColor="@color/black"
                android:textSize="15sp"
                tools:ignore="TouchTargetSizeCheck"/>

        </LinearLayout>

    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/e32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/e28"
        android:text="Actions"
        android:textStyle="bold"
        android:textSize="18sp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="15dp" />

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/e33"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:cardElevation="10dp"
        android:layout_below="@id/e32"
        android:layout_marginLeft="45dp"
        android:layout_marginRight="45dp">


        <LinearLayout
            android:id="@+id/e34"
            android:layout_width="match_parent"
            android:weightSum="1"
            android:orientation="horizontal"
            android:background="@color/semi_gray_color"
            android:layout_height="45dp">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/e35"
                android:src="@drawable/baseline_outlined_flag_24"
                android:layout_weight="0.1"/>

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="0.2"
                android:id="@+id/e36"
                android:backgroundTint="@color/transparent"
                android:text="Report a Problem"
                android:textColor="@color/black"
                android:textSize="15sp"
                tools:ignore="TouchTargetSizeCheck"/>

        </LinearLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/e37"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:cardElevation="10dp"
        android:layout_below="@id/e33"
        android:layout_marginLeft="45dp"
        android:layout_marginRight="45dp">


        <LinearLayout
            android:id="@+id/e38"
            android:layout_width="match_parent"
            android:weightSum="1"
            android:orientation="horizontal"
            android:background="@color/semi_gray_color"
            android:layout_height="45dp">

            <ImageView
                android:layout_width="40dp"
                android:layout_height="wrap_content"
                android:id="@+id/e39"
                android:src="@drawable/vec1"
                android:layout_marginLeft="4dp"
                android:background="@color/transparent"
                android:layout_weight="0.1"/>

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="0.2"
                android:id="@+id/e40"
                android:backgroundTint="@color/transparent"
                android:text="Add Account"
                android:textColor="@color/black"
                android:textSize="15sp"
                tools:ignore="TouchTargetSizeCheck"/>

        </LinearLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/e41"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:cardElevation="10dp"
        android:layout_below="@id/e37"
        android:layout_marginLeft="45dp"
        android:layout_marginRight="45dp">


        <LinearLayout
            android:id="@+id/e42"
            android:layout_width="match_parent"
            android:weightSum="1"
            android:orientation="horizontal"
            android:background="@color/semi_gray_color"
            android:layout_height="45dp">

            <ImageView
                android:layout_width="40dp"
                android:layout_height="42dp"
                android:id="@+id/e43"
                android:src="@drawable/image2"
                android:layout_weight="0.1"/>

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="0.2"
                android:id="@+id/e44"
                android:backgroundTint="@color/transparent"
                android:text="Log Out"
                android:textColor="@color/black"
                android:textSize="15sp"
                tools:ignore="TouchTargetSizeCheck"/>

        </LinearLayout>

    </androidx.cardview.widget.CardView>

</RelativeLayout>